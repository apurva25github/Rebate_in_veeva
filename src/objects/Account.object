        <fullName>jj_rb_SAP_CustomerNo_is_numeric</fullName>
        <active>false</active>
        <description>SAP Customer No. should be numeric.</description>
        <errorConditionFormula>AND( OR( ISPICKVAL(Type, &apos;Retailer&apos;) , ISPICKVAL(Type, &apos;Home care provider&apos;) ) , NOT(isnumber( jj_rb_SAP_Customer_Number__c )))</errorConditionFormula>
