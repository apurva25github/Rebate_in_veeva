    <validationRules>
        <fullName>jj_rb_SAP_CustomerNo_is_numeric</fullName>
        <active>false</active>
        <description>SAP Customer No. should be numeric.</description>
        <errorConditionFormula>AND( OR( ISPICKVAL(Type, &apos;Retailer&apos;) , ISPICKVAL(Type, &apos;Home care provider&apos;) ) , NOT(isnumber( jj_rb_SAP_Customer_Number__c )))</errorConditionFormula>
        <errorDisplayField>jj_rb_SAP_Customer_Number__c</errorDisplayField>
        <errorMessage>SAP Customer No. should be numeric.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_SAP_VendorNo_is_numeric</fullName>
        <active>true</active>
        <description>SAP Vendor No. should be numeric.</description>
        <errorConditionFormula>AND( OR( ISPICKVAL(Type, &apos;Retailer&apos;) , ISPICKVAL(Type, &apos;Homecare provider&apos;) ) , NOT(isnumber(jj_rb_SAP_Vendor_Number__c)))</errorConditionFormula>
