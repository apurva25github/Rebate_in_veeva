    <validationRules>
        <fullName>jj_rb_Credit_dates_validation</fullName>
        <active>true</active>
        <description>Credit check Validity End should be greater than Credit check Validity From</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;Homecare Customer Data Request&apos; ,jj_rb_Credit_check_Validity_End__c &lt;  jj_rb_Credit_check_Validity_From__c)</errorConditionFormula>
        <errorDisplayField>jj_rb_Credit_check_Validity_From__c</errorDisplayField>
        <errorMessage>Credit check Validity End should be greater than Credit check Validity From</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_IMS_cust_ID_is_number</fullName>
        <active>true</active>
        <description>IMS customer number should be a number</description>
        <errorConditionFormula>AND(RecordType.Name = &apos;Retailer Customer Data Request&apos;, NOT( ISNUMBER( jj_rb_IMS_Customer_Id__c )))</errorConditionFormula>
        <errorDisplayField>jj_rb_IMS_Customer_Id__c</errorDisplayField>
        <errorMessage>IMS customer number should be a number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_IMS_prod_ID_is_number</fullName>
        <active>true</active>
        <description>IMS product number should be a number.</description>
        <errorConditionFormula>AND(RecordType.Name = &apos;Product Data Request&apos;, NOT( ISNUMBER( jj_rb_IMS_Product_ID__c ) ) )</errorConditionFormula>
        <errorDisplayField>jj_rb_IMS_Product_ID__c</errorDisplayField>
        <errorMessage>IMS product number should be a number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_SAP_CustomerNo_is_numeric</fullName>
        <active>true</active>
        <description>SAP Customer no should be a number.</description>
        <errorConditionFormula>AND(  OR(RecordType.Name  = &apos;Retailer Customer Data Request&apos;,RecordType.Name  = &apos;Homecare Customer Data Request&apos;)  ,NOT(isnumber(jj_rb_SAP_Customer_Number__c) ))</errorConditionFormula>
        <errorDisplayField>jj_rb_SAP_Customer_Number__c</errorDisplayField>
        <errorMessage>SAP Customer No. should be a number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_SAP_EanCode_is_numeric</fullName>
        <active>true</active>
        <description>SAP EAN Code should be a number.</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;Product Data Request&apos;,NOT(isnumber(jj_rb_SAP_EAN_Code__c) ))</errorConditionFormula>
        <errorDisplayField>jj_rb_SAP_EAN_Code__c</errorDisplayField>
        <errorMessage>SAP EAN Code should be a number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_SAP_MaterialNo_is_numeric</fullName>
        <active>true</active>
        <description>SAP Material No. should be a number.</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;Product Data Request&apos;,NOT(isnumber(jj_rb_SAP_Material_Code__c ) ))</errorConditionFormula>
        <errorDisplayField>jj_rb_SAP_Material_Code__c</errorDisplayField>
        <errorMessage>SAP Material No. should be a number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_SAP_VendorNo_is_numeric</fullName>
        <active>true</active>
        <description>SAP Vendor No. should be a number.</description>
        <errorConditionFormula>AND(  OR(RecordType.Name  = &apos;Retailer Customer Data Request&apos;,RecordType.Name  = &apos;Homecare Customer Data Request&apos;) ,NOT(isnumber(jj_rb_SAP_Vendor_Number__c) ))</errorConditionFormula>
        <errorDisplayField>jj_rb_SAP_Vendor_Number__c</errorDisplayField>
        <errorMessage>SAP Vendor No. should be a number.</errorMessage>
    </validationRules>
