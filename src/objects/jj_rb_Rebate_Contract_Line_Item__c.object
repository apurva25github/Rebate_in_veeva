    <validationRules>
        <fullName>jj_rb_EndDt_greater_than_StartDt</fullName>
        <active>true</active>
        <description>End date should be greater than the start date</description>
        <errorConditionFormula>jj_rb_End_Date__c  &lt;=  jj_rb_Start_Date__c</errorConditionFormula>
        <errorMessage>End date should be greater than the start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_Listprice_greater_Contractprice</fullName>
        <active>true</active>
        <description>For Homecare contracts, the list price should be greater than the contract price in the line item.</description>
        <errorConditionFormula>AND(OR( RecordType.Name = &apos;Homecare Contract&apos;,RecordType.Name= &apos;Retailer Contract&apos;),jj_rb_Contract_Price__c &gt; jj_rb_List_Price__c)</errorConditionFormula>
        <errorMessage>List price should be greater than contract price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_Rebate_Value_per_Unit</fullName>
        <active>true</active>
        <description>It Validates that the rebate value per unit is equal to the difference in list price and agreement price.</description>
        <errorConditionFormula>(jj_rb_Rebate__c   &lt;&gt; (jj_rb_List_Price__c  -  jj_rb_Contract_Price__c)) &amp;&amp;   RecordType.DeveloperName =&apos;jj_rb_Retailer_Rebate_contract&apos;</errorConditionFormula>
        <errorMessage>Rebate value should be the difference of List price and Contract price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>jj_rb_minimum_less_maximum_value</fullName>
        <active>true</active>
        <description>Minimum Volume should be less than maximum  Volume</description>
        <errorConditionFormula>jj_rb_Minimum_Volume__c &gt; jj_rb_Maximum_Volume__c</errorConditionFormula>
        <errorMessage>Minimum Volume should be less than maximum  Volume</errorMessage>
    </validationRules>
